{
    "name": "mscart/acl",
    "description": "Role Management",
    "license": "MIT",
    "authors": [
        {
            "name": "Totoianu Mihai",
            "email": "toto@wattmedia.ro",
            "homepage": "https://wattmedia.ro"
        }
    ],
    "homepage": "https://github.com/mscart/acl",
    "keywords": ["Laravel", "Acl"],
    "require": {
        "illuminate/support": "~5|~6"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MsCart\\Acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MsCart\\Acl\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MsCart\\Acl\\AclServiceProvider"
            ],
            "aliases": {
                "Acl": "MsCart\\Acl\\Facades\\Acl"
            }
        }
    }
}
